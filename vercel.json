{
  "buildCommand": "npm ci && npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm ci",
  "framework": "vite",
  "routes": [
    {
      "src": "/assets/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/[^.]+",
      "dest": "/"
    },
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/((?!assets/).*)",
      "dest": "/index.html"
    }
  ]
}